{% block tabs %}

<div class="row pm-table" id="clientedittabs">
{{urls['invoice']}}
    <div id="formSection">
        <button id="clientForm" onclick="show('clientForm', '{{urls["clients"]}}')" class="btn btn-secondary" style="margin:5px;">Client</button>
        <button id="institutionForm" onclick="show('institutionForm', '{{urls["institution"]}}')" class="btn btn-secondary" style="margin:5px;">Institution</button>
